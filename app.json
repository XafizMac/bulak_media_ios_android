{
  "expo": {
    "name": "Булак Медиа",
    "slug": "tasbihat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/backgrouds/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2E0A30"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hafizbeyyyy.tasbihat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#2E0A30"
      },
      "package": "com.hafizbeyyyy.tasbihat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4a52fd8d-fd2c-4fcb-af48-b90577e9eaa6"
      }
    },
    "owner": "hafizbeyyyy",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) uses your location to provide accurate prayer times based on your current region."
        }
      ],
      "expo-font"
    ]
  }
}
